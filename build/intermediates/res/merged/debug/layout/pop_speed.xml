<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/translucent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/white"
        android:orientation="vertical" >

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/pop_number_line_height"
            android:gravity="center"
            android:text="@string/set_speed"
            android:textColor="@color/patientinfo_base_item_rightpart_text"
            android:textSize="@dimen/login_selectuser_text_size" />

        <LinearLayout
            android:id="@+id/ll_pop_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="5dp" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/pop_number_line_height"
                    android:orientation="horizontal" >

                    <Button
                        android:id="@+id/btn_20"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="@dimen/pop_number_btn_margin"
                        android:layout_weight="1"
                        android:background="@drawable/btn_changehead_bg"
                        android:text="@string/number_20"
                        android:textColor="@color/patientinfo_base_item_rightpart_text"
                        android:textSize="@dimen/pop_number_btn_textsize" />

                    <Button
                        android:id="@+id/btn_30"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="@dimen/pop_number_btn_margin"
                        android:layout_weight="1"
                        android:background="@drawable/btn_changehead_bg"
                        android:text="@string/number_30"
                        android:textColor="@color/patientinfo_base_item_rightpart_text"
                        android:textSize="@dimen/pop_number_btn_textsize" />

                    <Button
                        android:id="@+id/btn_40"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="@dimen/pop_number_btn_margin"
                        android:layout_weight="1"
                        android:background="@drawable/btn_changehead_bg"
                        android:text="@string/number_40"
                        android:textColor="@color/patientinfo_base_item_rightpart_text"
                        android:textSize="@dimen/pop_number_btn_textsize" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/pop_number_line_height"
                    android:orientation="horizontal" >

                    <Button
                        android:id="@+id/btn_50"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="@dimen/pop_number_btn_margin"
                        android:layout_weight="1"
                        android:background="@drawable/btn_changehead_bg"
                        android:text="@string/number_50"
                        android:textColor="@color/patientinfo_base_item_rightpart_text"
                        android:textSize="@dimen/pop_number_btn_textsize" />

                    <Button
                        android:id="@+id/btn_60"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="@dimen/pop_number_btn_margin"
                        android:layout_weight="1"
                        android:background="@drawable/btn_changehead_bg"
                        android:text="@string/number_60"
                        android:textColor="@color/patientinfo_base_item_rightpart_text"
                        android:textSize="@dimen/pop_number_btn_textsize" />

                    <Button
                        android:id="@+id/btn_70"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="@dimen/pop_number_btn_margin"
                        android:layout_weight="1"
                        android:background="@drawable/btn_changehead_bg"
                        android:text="@string/number_70"
                        android:textColor="@color/patientinfo_base_item_rightpart_text"
                        android:textSize="@dimen/pop_number_btn_textsize" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/pop_number_line_height"
                    android:orientation="horizontal" >

                    <Button
                        android:id="@+id/btn_80"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="@dimen/pop_number_btn_margin"
                        android:layout_weight="1"
                        android:background="@drawable/btn_changehead_bg"
                        android:text="@string/number_80"
                        android:textColor="@color/patientinfo_base_item_rightpart_text"
                        android:textSize="@dimen/pop_number_btn_textsize" />

                    <Button
                        android:id="@+id/btn_90"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="@dimen/pop_number_btn_margin"
                        android:layout_weight="1"
                        android:background="@drawable/btn_changehead_bg"
                        android:text="@string/number_90"
                        android:textColor="@color/patientinfo_base_item_rightpart_text"
                        android:textSize="@dimen/pop_number_btn_textsize" />

                    <Button
                        android:id="@+id/btn_100"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="@dimen/pop_number_btn_margin"
                        android:layout_weight="1"
                        android:background="@drawable/btn_changehead_bg"
                        android:text="@string/number_100"
                        android:textColor="@color/patientinfo_base_item_rightpart_text"
                        android:textSize="@dimen/pop_number_btn_textsize" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="7"
                android:orientation="vertical" >

                <Button
                    android:id="@+id/btn_none"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/pop_number_btn_margin"
                    android:layout_weight="1"
                    android:background="@drawable/btn_changehead_bg"
                    android:text="@string/none_speed"
                    android:textColor="@color/patientinfo_base_item_rightpart_text"
                    android:textSize="@dimen/pop_number_btn_textsize" />

                <Button
                    android:id="@+id/btn_save"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/pop_number_btn_margin"
                    android:layout_weight="1"
                    android:background="@drawable/btn_changehead_bg"
                    android:text="@string/save"
                    android:textColor="@color/patientinfo_base_item_rightpart_text"
                    android:textSize="@dimen/pop_number_btn_textsize" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>